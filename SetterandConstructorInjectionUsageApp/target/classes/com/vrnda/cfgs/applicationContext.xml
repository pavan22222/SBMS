<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="st" class="com.vrnda.beans.Student">

	<constructor-arg name="id" value="1"></constructor-arg>
	<constructor-arg name="name" value="Pavan"></constructor-arg>
	<constructor-arg name="fee" value="10000.0f"></constructor-arg>
	
	<property name="address" value="Hyd"></property>
	<property name="standard" value="10"></property>
</bean>
</beans>
